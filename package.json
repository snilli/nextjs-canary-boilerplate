{
  "name": "nestjs-canary-boilerplate",
  "author": "snilli",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build",
    "start": "next start",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && yarn && yarn dev",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest -i",
    "test:clear": "jest --clearCache",
    "test-all": "yarn lint && yarn type-check && yarn src/tests",
    "storybook": "start-storybook -p 6666",
    "build-storybook": "build-storybook",
    "analyze": "cross-env ANALYZE=true yarn build",
    "update-package": "ncu -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/material": "^5.1.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "firebase": "^9.4.1",
    "firebase-admin": "^10.0.0",
    "immer": "^9.0.6",
    "nanoid": "^3.1.31",
    "next": "^12.0.4",
    "phin": "^3.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "use-immer": "^0.6.0",
    "use-persisted-state": "^0.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@next/bundle-analyzer": "^12.0.4",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/react": "6.3.12",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/use-persisted-state": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.0.2",
    "npm-check-updates": "^12.0.2",
    "prettier": "^2.4.1",
    "react-docgen-typescript": "^2.1.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "jest": {
    "roots": [
      "src",
      "libs"
    ],
    "projects": [
      "src/jest.config.js",
      "libs/jest.config.js"
    ]
  }
}
