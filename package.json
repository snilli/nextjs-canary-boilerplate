{
  "name": "nestjs-canary-boilerplate",
  "author": "snilli",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build",
    "start": "next start",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && yarn && yarn dev",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "next lint",
    "test": "jest -i",
    "test:clear": "jest --clearCache",
    "test-all": "yarn lint && yarn type-check && yarn src/tests",
    "storybook": "start-storybook -p 6666",
    "build-storybook": "build-storybook",
    "analyze": "cross-env ANALYZE=true yarn build",
    "update-package": "ncu -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^2.8.8",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "autoprefixer": "^10.4.2",
    "chart.js": "^3.7.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "firebase": "^9.6.4",
    "firebase-admin": "^10.0.2",
    "immer": "^9.0.12",
    "joi": "^17.6.0",
    "moment": "^2.29.1",
    "nanoid": "^3.2.0",
    "next": "^12.0.9",
    "next-auth": "^4.1.2",
    "phin": "^3.6.1",
    "postcss": "^8.4.5",
    "prisma": "^3.8.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.1",
    "react-perfect-scrollbar": "^1.5.8",
    "reflect-metadata": "^0.1.13",
    "swr": "^1.2.0",
    "tailwindcss": "^3.0.17",
    "tsyringe": "^4.6.0",
    "use-immer": "^0.6.0",
    "use-persisted-state": "^0.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@next/bundle-analyzer": "^12.0.9",
    "@next/eslint-plugin-next": "^12.0.9",
    "@storybook/addon-essentials": "6.4.14",
    "@storybook/addon-links": "6.4.14",
    "@storybook/react": "6.4.14",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/chart.js": "^2.9.35",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/react": "^17.0.38",
    "@types/use-persisted-state": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-testing-library": "^5.0.4",
    "husky": "7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.3.2",
    "npm-check-updates": "^12.2.1",
    "prettier": "^2.5.1",
    "react-docgen-typescript": "^2.2.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "jest": {
    "roots": [
      "src",
      "libs"
    ],
    "projects": [
      "src/jest.config.js",
      "libs/jest.config.js"
    ]
  }
}
