{
  "name": "nestjs-canary-boilerplate",
  "author": "snilli",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev -p 3333",
    "build": "next build",
    "start": "next start",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf .next/ && yarn && yarn dev",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest -i",
    "test:clear": "jest --clearCache",
    "test-all": "yarn lint && yarn type-check && yarn src/tests",
    "storybook": "start-storybook -p 6666",
    "build-storybook": "build-storybook",
    "analyze": "cross-env ANALYZE=true yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@hookform/resolvers": "^1.3.7",
    "@material-ui/core": "^5.0.0-alpha.28",
    "@material-ui/icons": "^5.0.0-alpha.28",
    "chart.js": "^2.9.4",
    "clsx": "^1.1.1",
    "firebase": "^8.3.1",
    "firebase-admin": "^9.6.0",
    "immer": "^9.0.1",
    "joi": "^17.4.0",
    "moment": "^2.29.1",
    "nanoid": "^3.1.22",
    "next": "^11.1.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-hook-form": "^6.15.5",
    "react-perfect-scrollbar": "^1.5.8",
    "reflect-metadata": "^0.1.13",
    "swr": "^0.5.5",
    "tsyringe": "^4.5.0",
    "use-immer": "^0.5.1",
    "use-persisted-state": "^0.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@next/bundle-analyzer": "^10.1.2",
    "@storybook/addon-essentials": "6.2.1",
    "@storybook/addon-links": "6.2.1",
    "@storybook/react": "6.2.1",
    "@testing-library/react": "^11.2.6",
    "@types/chart.js": "^2.9.31",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/use-persisted-state": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-docgen-typescript": "^1.21.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "jest": {
    "roots": [
      "src",
      "libs"
    ],
    "projects": [
      "src/jest.config.js",
      "libs/jest.config.js"
    ]
  }
}
